<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">
    <!-- Note: attributes are set twice - app is for API 16->25, android is for API 26   -->
    <font
        android:font="@font/ibm_plex_sans_light"
        android:fontStyle="normal"
        android:fontWeight="300"

        app:font="@font/ibm_plex_sans_light"
        app:fontStyle="normal"
        app:fontWeight="300" />
    <font
        android:font="@font/ibm_plex_sans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/ibm_plex_sans_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />
    <font
        android:font="@font/ibm_plex_sans_medium"
        android:fontStyle="normal"
        android:fontWeight="500"

        app:font="@font/ibm_plex_sans_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />
</font-family>